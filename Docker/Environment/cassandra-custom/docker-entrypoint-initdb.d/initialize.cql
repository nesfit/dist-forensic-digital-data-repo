
/* KeySpace for structured data, can contain more tables. */
CREATE KEYSPACE IF NOT EXISTS structured_data WITH replication = { 'class':'SimpleStrategy', 'replication_factor':1 };
USE structured_data;

/* Table for PCAP files, column packet contains one packet from PCAP. Id is generated by function now(). */
CREATE TABLE IF NOT EXISTS pcap (
	id timeuuid PRIMARY KEY,
	packet ascii
);